{
  "name": "taskflow-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "imports": {
    "#root/*": "./*.mjs",
    "#src/*": "./src/*.mjs",
    "#operators/*": "./src/Operators/*.mjs",
    "#services/*": "./src/Services/*.mjs",
    "#shared/*": "./src/shared/*.mjs"
  },
  "scripts": {
    "start": "nodemon --watch .env --watch . --ignore 'src/shared/task*Schema.json' --ignore 'src/shared/fsm' --ignore 'db' server",
    "debug": "nodemon --watch .env --watch . --ignore 'src/shared/task*Schema.json' --ignore 'src/shared/fsm' --ignore 'db' --trace-warnings --inspect=${DEBUG_PORT:-0.0.0.0:9229} server.js"
  },
  "dependencies": {
    "@keyv/redis": "^2.7.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs": "^4.4.0",
    "@tensorflow/tfjs-node": "^4.4.0",
    "async-mutex": "^0.4.0",
    "bcrypt": "^5.1.1",
    "beaver-logger": "^4.0.34",
    "better-sqlite3": "^8.1.0",
    "chatgpt": "^5.1.3",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-request-id": "^3.0.0",
    "express-ws": "^5.0.2",
    "gpt-3-encoder": "^1.1.4",
    "http": "^0.0.1-security",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^2.0.6",
    "iconv-lite": "^0.6.3",
    "intl": "^1.2.5",
    "json-diff": "^1.0.6",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^4.5.3",
    "keyv-better-sqlite3": "^1.0.0",
    "keyv-redis": "^1.3.4",
    "lodash": "^4.17.21",
    "mongodb": "^5.1.0",
    "mongoose": "^7.5.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.21",
    "redis": "^4.6.8",
    "seedrandom": "^3.0.5",
    "sqlite3": "^5.1.6",
    "ws": "^8.12.1",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "quicktype": "^23.0.30"
  }
}
